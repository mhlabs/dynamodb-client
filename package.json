{
  "name": "@mhlabs/dynamodb-client",
  "version": "3.3.0",
  "description": "A lib to simplify working with dynamodb document client in aws sdk v3.",
  "main": "dist/index.cjs.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest ./src",
    "coverage": "jest ./src --coverage",
    "integration-tests": "jest ./integration-tests",
    "prebuild": "rm -rf ./dist",
    "build": "node ./scripts/build.js",
    "postbuild": "tsc --emitDeclarationOnly --declaration",
    "compile": "tsc"
  },
  "keywords": [
    "aws",
    "dynamodb",
    "v3",
    "documentclient"
  ],
  "author": "mhlabs",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.165.0",
    "@aws-sdk/lib-dynamodb": "^3.165.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^29.0.1",
    "aws-sdk-client-mock": "^2.0.0",
    "esbuild": "^0.16.16",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhlabs/dynamodb-client.git"
  },
  "bugs": {
    "url": "https://github.com/mhlabs/dynamodb-client/issues"
  },
  "homepage": "https://github.com/mhlabs/dynamodb-client#readme"
}
